public class Prob1D0117
{
	public static void main(String[] args) 
	{
		int n = 15;
		int sum =0;
		int pre_value=1;

		for (int i=0; i<n ; i++)
		{
			pre_value += i;      
			sum += pre_value;           
			
		}

		System.out.println(n + "번째까지의 합:"+sum);

	}//end of main
}//end of class
